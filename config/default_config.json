{
    "processing": {
      "vector_dimension": 10000,
      "vector_type": "binary",
      "reasoning_approach": "majority",
      "certainty_propagation": "min",
      "recalibration_enabled": true,
      "max_reasoning_depth": 10,
      "domain_config": {
        "positive_outcome_keywords": ["positive", "increase", "growth"],
        "negative_outcome_keywords": ["negative", "decrease", "decline"],
        "neutral_outcome_keywords": ["neutral", "stable", "unchanged"],
        "outcome_field": "recommendation",
        "positive_outcome": "POSITIVE",
        "negative_outcome": "NEGATIVE",
        "neutral_outcome": "NEUTRAL"
      }
    },
    "persistence": {
      "load_previous_state": false,
      "save_state": true,
      "state_save_path": "./output/state.pkl"
    },
    "logging": {
      "log_level": "info",
      "log_path": "./logs/hyperlogica.log",
      "include_vector_operations": true,
      "include_llm_interactions": true,
      "include_reasoning_steps": true
    },
    "llm": {
      "model": "gpt-4",
      "temperature": 0.0,
      "max_tokens": 2000
    },
    "output_schema": {
      "format": "json",
      "fields": [
        {"name": "entity_id", "type": "string"},
        {"name": "entity_name", "type": "string"},
        {"name": "recommendation", "type": "string"},
        {"name": "certainty", "type": "float"},
        {"name": "reasoning", "type": "object"}
      ],
      "include_reasoning_trace": true,
      "include_explanation": true,
      "include_vector_details": false
    }
  }